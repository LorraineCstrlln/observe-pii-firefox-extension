{
  "manifest_version": 2,
  "name": "DualHead Contextual Detector",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "lib/ort.min.js",
    "lib/ort-wasm.wasm",
    "model/dualhead.onnx"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' blob:; object-src 'self';"
}
